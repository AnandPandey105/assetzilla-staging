                                                            function unitConverter(areas, currentUnit, idx, changeInto) {
                                                              // console.log('unitConverter called', currentUnit, changeInto)
                                                              var res;
                                                              for (var i = 0; i <= areas.length; i++) {
                                                                val = Number(areas[i].area);
                                                                var landArea = areas[i].land_area;
                                                                var apartment_area = areas[i].apartment_area;

                                                                // Acre
                                                                if (currentUnit == "acre") {
                                                                  if (changeInto == "sq ft") {
                                                                    res = val;
                                                                  } else if (changeInto == "sq ft") {
                                                                    res = val * 43560;
                                                                    if (landArea) landArea = landArea * 43560;
                                                                    if (apartment_area) apartment_area = apartment_area * 43560;

                                                                  } else if (changeInto == "sq yards") {
                                                                    res = val * 4840;
                                                                    if (landArea) landArea = landArea * 4840;
                                                                    if (apartment_area) apartment_area = apartment_area * 4840;

                                                                  } else if (changeInto == "sq meter") {
                                                                    res = val * 4046.856;
                                                                    if (landArea) landArea = landArea * 4046.856;
                                                                    if (apartment_area) apartment_area = apartment_area * 4046.856;

                                                                  }
                                                                }
                                                                // Sq feet
                                                                else if (currentUnit == "sq ft") {
                                                                  if (changeInto == "sq ft") {
                                                                    res = val;;
                                                                  } else if (changeInto == "acre") {
                                                                    res = val / 43560;

                                                                    if (landArea) landArea = landArea / 43560;
                                                                    if (apartment_area) apartment_area = apartment_area / 43560;

                                                                  } else if (changeInto == "sq yards") {
                                                                    res = val / 9;
                                                                    if (landArea) landArea = landArea / 9;
                                                                    if (apartment_area) apartment_area = apartment_area / 9;

                                                                  } else if (changeInto == "sq meter") {

                                                                    res = val / 10.764;
                                                                    if (landArea) landArea = landArea / 10.764;;
                                                                    if (apartment_area) apartment_area = apartment_area / 10.764;;
                                                                  }
                                                                }
                                                                // Sq yards
                                                                else if (currentUnit == "sq yards") {
                                                                  if (changeInto == "sq yards") {
                                                                    res = val;;
                                                                  } else if (changeInto == "acre") {
                                                                    res = val / 4840;

                                                                    if (landArea) landArea = landArea / 4840;
                                                                    if (apartment_area) apartment_area = apartment_area / 4840;

                                                                  } else if (changeInto == "sq ft") {
                                                                    res = val / 9;
                                                                    if (landArea) landArea = landArea / 9;
                                                                    if (apartment_area) apartment_area = apartment_area / 9;


                                                                  } else if (changeInto == "sq meter") {
                                                                    res = val / 1.196;

                                                                    if (landArea) landArea = landArea / 1.196;
                                                                    if (apartment_area) apartment_area = apartment_area / 1.196;
                                                                  }
                                                                }
                                                                // Sq meter
                                                                else if (currentUnit == "sq meter") {
                                                                  if (changeInto == "sq meter") {
                                                                    res = val;;
                                                                  } else if (changeInto == "acre") {
                                                                    res = val / 4046.856;
                                                                    if (landArea) landArea = landArea / 4046.856;
                                                                    if (apartment_area) apartment_area = apartment_area / 4046.856;

                                                                  } else if (changeInto == "sq ft") {
                                                                    res = val * 10.764;

                                                                    if (landArea) landArea = landArea * 10.764;
                                                                    if (apartment_area) apartment_area = apartment_area * 10.764;

                                                                  } else if (changeInto == "sq yards") {
                                                                    res = val * 81.196;

                                                                    if (landArea) landArea = landArea * 81.196;
                                                                    if (apartment_area) apartment_area = apartment_area * 81.196;

                                                                  }
                                                                }
                                                                document.getElementById('area-' + i)
