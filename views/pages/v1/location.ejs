                            <br>
                          <small style="word-break:break-word;">
                            <% if (data.top_city.results[i].total_projects){%>
                            <%= data.top_city.results[i].total_projects %> Projects
                            <% } %>

                            <% if (data.top_city.results[i].price && data.top_city.results[i].price.min){%>
                            starting
                            <%= changeNumberFormat(data.top_city.results[i].price.min) %>
                            <%} else {%>
                            <% } %>
                          </small>
                          <% } %>
                          </a>
                        <% } %>
                    </div>
                  </div>
                <%}%>
                <div class="col-6 col-md-6 book-property-btn-wrp">
                  <button class="btn btn-book-prop" style="margin-left: 0px; height:48px;" onclick="scrollto('#allProjectsDiv')">View Projects</button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
      </div>

      <div class="row w-100 mx-0">
