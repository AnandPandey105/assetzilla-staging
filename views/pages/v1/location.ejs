                            <% if (location.state){%>
                              <a class="text-left text-white" href="<%=location.state.url%>" target="_blank">
                                <%= location.state.name %>
                              </a>
                            <%}%>
                            <% if (location.district && location.district.name!=="Mumbai Suburban"){%>
                              > 
                              <a class="text-left text-white" href="<%=location.district.url%>" target="_blank">
                                <%= location.district.name %>
                              </a>
                            <%}%>
                            <% if (location.city){%>
                              > 
                              <a class="text-left text-white" href="<%=location.city.url%>" target="_blank">
                                <%= location.city.name %>
                              </a>
                            <%}%>
                          </div>
                        </p>
                      <%}%>
                    <%}%>
                  </div>
