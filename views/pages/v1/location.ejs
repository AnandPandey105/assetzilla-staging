      <div class="details-box-wrapper mt-4">
        <div class="container">
          <div class="row">
            <div class="col-lg-8">

              <% if (data.location_details){ %>
              <div class="common-box-dt">
                <div class="dt-common-head">
                  <h2>Details of <%= data.name %></h2>
                </div>
                <div class="">
                  <p><%- data.location_details %></p>
                </div>
              </div>
