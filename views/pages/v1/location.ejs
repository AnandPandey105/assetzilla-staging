                  <% } %>
                  <% if (data.capital_income){ %>
                    <p>
                      <span class="font-weight-600">Per Capita Income :</span> 
                        <%- data.capital_income %> (Rs)
                      </p>
                  <% } %>
                  <% if (data.population){ %>
                    <p>
                      <span class="font-weight-600">Population :</span>
                        <%- data.population %> (Lakhs)
                    </p>
                  <% } %>
                </div>
              </div>
