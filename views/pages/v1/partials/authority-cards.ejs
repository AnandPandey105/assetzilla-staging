<% for(var i=0; i < results.length; i++) { %>
    <div class="col-lg-3 col-sm-4 col-12 custom-mb-30">
        <div class="appartement-grid-box" >
            <div class="appartment-checkbox lc-grid-checkbox-new">
                <div class="custom-control custom-checkbox">
                   <input 
                      type="checkbox" 
                      class="custom-control-input add-to-comare"
                      id="example-<%=results[i].id%>" 
                      name="example-<%=results[i].id%>"
                      onchange="add_to_compare('<%=JSON.stringify(results[i])%>')"
                   >
                   <label class="custom-control-label"
                      for="example-<%=results[i].id%>">
                   </label>
                </div>
             </div>
            <div class="app-grid-img auth-logo-img" onclick=go_to_page("<%=results[i].url %>")>
                <img class="img img-fluid" src="<%=results[i].logo %>" alt="">
            </div>
            <div class="location-grid-content auth-grid-content">
                <div class="lc-content-row">
                    <div class="row align-items-center">
                        <div class="col-12">
                            <div class="text-center">
                                <h2>
                                    <%- include('./bookmark-list', {url: results[i].url, is_bookmarked: results[i].is_bookmarked, id: i + "_authority-" + results[i].id}); %>
                                    <span onclick=go_to_page("<%=results[i].url %>")><%=results[i].name %> </span>
                                 </h2>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="text-center">
                                <% if (results[i].state || results[i].district){ %>
                                    <p class="auth-location-font"><img src="/v1/img/gps.webp" alt=""> 
                                        <% if (results[i].district){ %>
                                            <%=results[i].district %>
                                        <%}%>
                                        <% if (results[i].state && results[i].district){ %>
                                            , 
                                        <%}%>
                                        <% if (results[i].state){ %>
                                            <%=results[i].state %>
                                        <%}%>
                                    </p>
                                <%}%>  
                              
                            </div>
                        </div>
                    </div>    
                </div> 

                <div class="lc-content-dt auth-content-dt ">
                    
                    <div class="row mb-0">
                        <div class="col-12">
                            <% if (results[i].price && results[i].price.min){ %>
                                <p class="text-center border-bottom">Properties starting from : <b> <% if (results[i].price && results[i].price.min){ %>
                                    <%=changeNumberFormat(results[i].price.min) %>
                                <%} else {%>
                                        - 
                                <%}%></b> </p>
                            <%}%>
                        </div>
                    </div>

                    <div class="row align-items-center mb-0">
                        <div class="col-4">
                            <div class="text-center">
                            <p>Total 
                                Projects</p>
                                <h6>
                                    <% if (results[i].total_projects){ %>
                                        <%=results[i].total_projects %>
                                    <%} else {%>
                                            - 
                                    <%}%>
                                </h6>
                            </div>
                           
                             
                        </div>

                        <div class="col-4">
                            <div class="text-center">
                                <p class="">Under 
                                    Const. </p>
                                    <h6>
                                        <% if (results[i].project_status_count && results[i].project_status_count.UnderConstruction){ %>
                                            <%=results[i].project_status_count.UnderConstruction %>
                                        <%} else {%>
                                                - 
                                        <%}%>
                                    </h6>
                                </div>
                        </div>

                        <div class="col-4">
                            <div class="text-center">
                                <p class="">Ready to
                                    move</p>
                                    <h6>
                                        <% if (results[i].project_status_count && results[i].project_status_count.ReadyToMove){ %>
                                            <%=results[i].project_status_count.ReadyToMove %>
                                        <%} else {%>
                                                - 
                                        <%}%>
                                    </h6>
                                </div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>
    </div>
<% } %>
<div class="col-12 no-login-msg-div">
    <h3 class="no-login-msg see_more_results w-100">
      <span>
        <p class="btn blue-text w-100" onclick="see_more_results()">
          See more <img class="up_down_arrow" src="/images/down_arrow.svg" alt="down arrow" />
        </p>
      </span>
    </h3>
  </div>
