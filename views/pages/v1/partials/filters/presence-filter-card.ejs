<% for(var i=0; i < presence.length; i++) { %>
    <li class="popup" id="li-<%=presence[i].nameAsURL %>">
        <div class="custom-control custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="<%=presence[i].nameAsURL %>"  onchange="handleFilters('presence','<%=presence[i].presence %>')"  autocomplete="off">
            <label class="custom-control-label" for="<%=presence[i].nameAsURL %>"><%=presence[i].presence %> (<%=presence[i].count %>)</label>
        </div>
    </li>
<% } %> 

<% for(var i=0; i < presence.length; i++) { %>
    <li class="quick" id="quick-li-<%=presence[i].nameAsURL %>">
        <div class="custom-control custom-checkbox">
            <input type="checkbox" class="custom-control-input" id="quick-<%=presence[i].nameAsURL %>" onchange="handleFilters('presence','<%=presence[i].presence %>')" autocomplete="off">
            <label class="custom-control-label" for="quick-<%=presence[i].nameAsURL %>"><%=presence[i].presence %> (<%=presence[i].count %>)</label>
        </div>
    </li>
<% } %> 

