<% for(var i=0; i < results.length; i++) { %>
  <div class="left-con-wrap">

    <div>
      <div class="apparment-list apparment-list-updated no-border" id="<%=i+custSkip%>_property-<%=results[i].id%>" onmouseenter="showBox('<%=results[i].id%>', <%=i%>, <%=i+custSkip%>, '<%=results[i].price.price%>', '<%=results[i].url%>')" onmouseleave="hideBox('<%=results[i].id%>', <%=i%>, <%=i+custSkip%>)">
        <div class="apparment-list-left">
          <a href="javascript:;" class="add-com-btn">
            <div class="appartment-checkbox">
              <div class="custom-control custom-checkbox">
                <input type="checkbox" class="custom-control-input add-to-comare" id="example-<%=results[i].id%>" name="example-<%=results[i].id%>" onchange="add_to_compare('<%=JSON.stringify(results[i])%>')">
                <label class="custom-control-label" for="example-<%=results[i].id%>">
                </label>
              </div>
            </div>
          </a>
          <div class="owl-carousel owl-theme allProp-first-owl" onclick="go_to_page('<%=results[i].url%>')">
            <% if(results[i].banner_image=="/images/placeholder_bg.webp" && results[i].images && results[i].images.Projects && results[i].images.Projects[0]){ %>
              <% for(var j=0; j <results[i].images.Projects.length; j++) { %>
                <div class="item">
                  <div class="all-prop-heart">
                    <img src="<%=locals.CURRENT_IMAGE_BASE_URL%>/project/image/<%=results[i].images.Projects[j]%>" class="img img-fluid">
                  </div>
                </div>
              <% } %>
            <%} else {%>
              <div class="item">
                <div class="all-prop-heart">
                  <img src="<%=results[i].banner_image %>" class="img img-fluid">
                </div>
              </div>
            <% } %>
            
          </div>
          <% if (emi_per_lac && (results[i].price && results[i].price.price)) { %>
            <p class="text-left emi_starting_block">
              EMI Starting :
              <strong class="pe-2 text-left">
                ₹<%=locals.formatToIndianNumber.format(Math.round(emi_per_lac * results[i].price.price / 100000))%>
              </strong>
              <button type="button" class="pe-2 btn btn-sm emi-tooltip btn-secondary" data-toggle="tooltip" data-html="true" title="<b>Down payment: 20%<br/>Loan for 25 yrs</b>">
