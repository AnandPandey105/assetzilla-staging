            <div class="loc-left">
              <i class="fa fa-map-marker mr-2" aria-hidden="true"></i>
              <p> <% if (results[i].subcity){ %>
                <%=results[i].subcity %>,
                <%}%>
                <% if (results[i].city){ %>
                <%=results[i].city %>
                <%}%>
              </p>
            </div>
            <div class="area_prices">
              <h2>
                <% if (results[i].area && results[i].area.area){ %>
                  <%=results[i].area.area %> sq ft.
                <%} else {%>
                -
                <%}%>
              </h2>
            </div>
          </div>
          <hr>
          <div class="featured-points">
            <ul>
              <% if (results[i].bhk_space){ %>
              <li>
                <h5>
                  <%=results[i].bhk_space %>
                </h5>
              </li>
              <%}%>
              <% if (results[i].property_type){ %>
