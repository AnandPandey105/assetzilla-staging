<div class="item">
  <div class="home-apartment" onclick="go_to_page('<%=results.results.locations[i].url%>')">
    <div class="ha-img-cities">
      <img src="<%=results.results.locations[i].banner_image%>" class="img img-fluid-custom">
    </div>
    <div class="ha-content location_info">
      <h2 class="city-name">
        <%=results.results.locations[i].name%>
        <p class="llt mb-0" 
          style="
          text-transform: uppercase;
          font-size: 12px;
          font-weight: lighter;
          padding: 5px;
          background: khaki;
          border-radius: 8px;
          width: fit-content;
          line-height: 100%;"
        >
          <%=results.results.locations[i].location_type%>
        </>
      </h2>
      <p>
        <% if (results.results.locations[i].total_projects ){ %>
          <span class="city-projects">
            <%=results.results.locations[i].total_projects %> Projects
          </span><br />
        <% }else{ %>
        <% } %>
        <% if (results.results.locations[i].price && results.results.locations[i].price.min ){ %>
          <span class="city-starting">Starting 
            <strong>
              <%= changeNumberFormat(results.results.locations[i].price.min) %>
            </strong>
        <% }else{ %>
        <% } %>
      </p>
    </div>
  </div>
</div>