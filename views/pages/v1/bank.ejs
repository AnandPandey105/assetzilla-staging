<%- include ./partials/header.ejs%>

<body>
  <%- include ./partials/navigation.ejs%>
  <div class="common-wrapper">
    <div class="details-breadcrumb pb-25 details-breadcrumb-img">
      <div class="container">
        <div class="dbread-top">
          <div class="row align-items-center">
            <% if(data.is_live==="1") {%>
