            <% } else if ( data.is_live==="4" || data.is_live==="3"){ %>
              <div class="notLive"> This location is not live anymore </div>
            <% }%>
            <div class="col-lg-8 col-sm-12">
              <div class="dt-bread-crumb-left">
                <div class="builder-top-logo">
