<!DOCTYPE html>
<html lang="en">

<%- include ./partials/header.ejs%>

  <body>
    <%- include ./partials/navigation.ejs%>
    <%- include ./partials/topbar_info_pages.ejs%>

    <div class="top-margin">
      <div class="container background-texture">
        <div class="container-thankyou">
          <img class="like" src="/v1/img/like.png" />
          <img src="/v1/img/trngl.webp" class="img img-fluid spin-img trngl-spin"  style="left:28%">
          <!-- <img src="/v1/img/logo.webp"/> -->
          <h1> Thank you for submitting your property details! </h1>
          <h4> We will review the information you provided.<br/>Please log in to your dashboard to check the status of verification.</h4>
          <p>Redirecting you to Home Page
            <span id="timer" style="color:blue">00:10</span>
          </p>
          <!-- <a href="<%=backTo%>">Click here to go back</a> -->
          <a class="btn btn-sm btn-primary" href="/">Go To Home</a>
        </div>
      </div>
    </div>

    <%- include ./partials/footer.ejs%>

  <script>
    let time = 10;
    setInterval(() => {
      if (time > 0)
        time--;
      document.getElementById('timer').innerText = '00:0' + time;
    }, 1000);

    setTimeout(() => {
      //window.location = "<%=backTo%>";
      window.location = "/";
    }, 10000)
  </script>
</body>

</html>