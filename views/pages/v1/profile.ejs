      .val();
    let passvalid = new RegExp("^(?=.*[A-Z])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{6,16}$")
    if (!(passvalid.test(password))) {
      customAlert("Invalid Password");
      return
    }
    if (password != password_re) {
      customAlert("Passwords do not match");
      return
    }
    $.post('/api/customer/change_password', {
      password: password
    }, function(data) {
      if (data.success) {
