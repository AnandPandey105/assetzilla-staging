                    <div>
                      <h5>Green Amenities :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.green_amenites_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b>
                      </h5>
                    </div>
