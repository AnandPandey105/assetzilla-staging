              <h2 style="display: flex; align-items: start;">
                <span class="d-flex align-items-center mr-0 pt-2"><%- include ./partials/bookmark.ejs%></span>
                <span class="<% if (locals.user) {%>ml-0<%} else {%>ml-1<%}%>"><%= data.name %></span>
              </h2>
              
              <div class="d-flex flex-column flex-lg-row align-items-start align-items-lg-center">
                <div style="display: flex;" class="justify-content-start align-items-start">
                  <p class="">
                    <img src="/v1/img/gps.webp" alt="" />
                    <div class="text-white ml-1">
                    <% if (location.subcity){%>
                    <a class="clr-white text-white" href="<%=location.subcity.url%>" target="_blank">
                      <%= location.subcity.name %>
                    </a>,
                    <%}%>
                    <% if (location.city){%>
                    <a class="clr-white text-white" href="<%=location.city.url%>" target="_blank">
                    <%= location.city.name %>
                    </a>,
                    <%}%>
                    <% if (location.district){%>
                    <a class="clr-white text-white" href="<%=location.district.url%>" target="_blank">
                    <%= location.district.name %>
                    </a>,
                    <%}%>
                    <% if (location.state){%>
                    <a class="clr-white text-white" href="<%=location.state.url%>" target="_blank">
                    <%= location.state.name %>
