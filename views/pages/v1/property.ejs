              <% if (project.other_amenites_json && project.other_amenites_json.length>0 && project.other_amenites_json.filter(x => typeof x === 'string' && x.length > 0).length > 0){ %>
                <div class="row mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Other Amenities :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.other_amenites_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b></h5>
