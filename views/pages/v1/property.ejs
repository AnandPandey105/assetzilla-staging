  </div>
  <div class="details-box-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <div class="common-box-dt">
            <div class="dt-common-head">
              <h2>Description</h2>
            </div>
            <div class="">
              <% if (data.description){ %>
              <h6>
                <%- data.description %>
              </h6>
              <%}%>
            </div>
            <% if (builder.length> 0){ %>
              <div class="row mt-4 pt-2 mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Builder :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <a href="<%= builder[0].url %>" target="_blank">
                      <img class="img img-fluid desc-logo-img" src="<%=locals.CURRENT_IMAGE_BASE_URL%>/builder/<%= builder[0].logo %>" alt="">
                    </a>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (authority.length>0){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Approved by : </h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5>
                      <b>
                        <a href="<%= authority[0].url %>" target="_blank">
                          <%= authority[0].name %>
                        </a>
                      </b>
                    </h5>
                    <!-- <img class="img img-fluid desc-logo-img" src="/v1/img/noida-logo.webp" alt=""> -->
                  </div>
                </div>
              </div>
            <%}%>
            <% if (project.name){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Project Name :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b><a href="<%= project.url %>" target="_blank">
                          <%= project.name %>
                        </a></b></h5>
                  </div>
                </div>
              </div>
              <%}%>
          </div>
          <div class="common-box-dt">
            <div class="dt-common-head">
              <h2>Property Details</h2>
            </div>
            <% if (data.property_type){ %>
              <div class="row mt-4 pt-2 mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Property Type :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b> <%= data.property_type %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>   
            <% if (data.area && data.area.area){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Area :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div class="d-flex flex-row justify-content-start align-items-center">
                    <h5 class="m-0"><b id="area-id"> <%= data.area.area %> </b></h5>
                    <select class="mx-2 unit-changer" id="convert-to" onchange="unitConverter(<%=JSON.stringify(data.area.area)%>, 'sq ft', 'area-id', this.value )">
                      <option selected>sq ft</option>
                      <option>acre</option>
                      <option>sq yards</option>
                      <option>sq meter</option>
                    </select>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.expected_completion){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Possession Year :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b id="expected_completion"></b></h5>
                  </div>
                </div>
              </div>
            <%}%>    
            <% if (data.tower_number){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Tower No. :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b> <%= data.tower_number %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.bhk_space){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>BHK :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b> <%= data.bhk_space %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.condition){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Condition :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b><%= data.condition %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.facing){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Facing :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b><%= data.facing %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.furnished){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Furnished Status :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b> <%= data.furnished %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>    
            <% if (data.ownership){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Ownership :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b><%= data.ownership %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>
            
            <% if ((data.floor || data.floor === 0) && false){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Floor : </h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b id="floorno"> </b></h5>
                  </div>
                </div>
              </div>
            <%}%>            
            <% if (data.loan_amount){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Loan Amount : </h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5><b> <%= changeNumberFormat(data.loan_amount) %></b></h5>
                  </div>
                </div>
              </div>
            <%}%>    
          </div>
          <% if (locals.project){ %>
            <div class="common-box-dt">
              <div class="dt-common-head">
                <h2>Project Details</h2>
                <%- console.log(locals.project) %>
              </div>
              <% if (project.project_status){ %>
                <div class="row mt-4 pt-2 mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Status :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.project_status %></b></h5>
                    </div>
                  </div>
                </div>
              <%}%>
              <% if (project.parking_details && project.parking_details.parking ){ %>
                <div class="row mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Visiter's Parking :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.parking_details.parking %></b></h5>
                    </div>
                  </div>
                </div>
              <%}%>
              <% if (project.amenities_json && project.amenities_json.length>0 && project.amenities_json.filter(x => typeof x === 'string' && x.length > 0).length > 0){ %>
                <div class="row mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Amenities :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.amenities_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b></h5>
                    </div>
                  </div>
                </div>
              <%}%>
              <% if (project.connectivity_amenites_json && project.connectivity_amenites_json.length>0 && project.connectivity_amenites_json.filter(x => typeof x === 'string' && x.length > 0).length > 0){ %>
                <div class="row mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Connectivity : </h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b><%= project.connectivity_amenites_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b></h5>
                    </div>
                  </div>
                </div>
