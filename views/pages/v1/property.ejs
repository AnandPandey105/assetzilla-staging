                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Kid's Amenities :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b> <%= project.kid_amenites_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b></h5>
                    </div>
                  </div>
                </div>
              <%}%>
              <% if (project.health_amenites_json && project.health_amenites_json.length>0 && project.health_amenites_json.filter(x => typeof x === 'string' && x.length > 0).length > 0){ %>
                <div class="row mb-3">
                  <div class="col-6  col-lg-3">
                    <div>
                      <h5>Health Amenities :</h5>
                    </div>
                  </div>
                  <div class="col-6 col-lg-9">
                    <div>
                      <h5><b><%= project.health_amenites_json.filter(x => typeof x === 'string' && x.length > 0).join(", ") %></b>
                      </h5>
                    </div>
                  </div>
                </div>
              <%}%> 
              <% if (project.green_amenites_json && project.green_amenites_json.length>0 && project.green_amenites_json.filter(x => typeof x === 'string' && x.length > 0).length > 0){ %>
                <div class="row mb-3">
