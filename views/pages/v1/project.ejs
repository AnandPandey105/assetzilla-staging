<script>
  $('input[type="date"]')
    .change(function() {
      // console.log($(this)
      //   .val()
      //   .length)
      if ($(this)
        .val()
        .length > 0) {
        document.getElementById('calender-value')
          .innerHTML = $(this)
          .val()
      } else if ($(this)
        .val()
        .length === 0) {
        document.getElementById('calender-value')
          .innerHTML = "YYYY-MM-DD"
      } else {
        document.getElementById('calender-value')
          .innerHTML = "YYYY-MM-DD"
      }
    });
</script>
