<!doctype html>
<html lang="en">

<head>
  <title>About
    <%= data.name %> in
    <% if (location.city){%>
    <%= location.city.name %>
    <%}%>,
            <% if (location.state){%>
    <%= location.state.name %>
    <%}%> with property type, prices & area | Assetzilla</title>
        <meta name="title" content="About <%= data.name %> in <% if (location.city){%>
    <%= location.city.name %><%}%>, <% if (location.state){%> <%= location.state.name %> <%}%> with property type, prices & area | Assetzilla">
        <meta name="description" content="View <%= data.property_type %> in <%= data.name %>, <% if (location.subcity){%>
    <%= location.subcity.name %><%}%>, <% if (location.city){%>
    <%= location.city.name %><%}%>.  Prices, offers, area, amenities for <%= data.property_type %> in
    <%= data.name %>. Visit Now">
    <script src="https://mozilla.github.io/pdf.js/build/pdf.js"></script>
</head>
<%- include ./partials/header.ejs%>

<body>
  <%- include ./partials/navigation.ejs%>
  <div class="common-wrapper">
    <div class="details-breadcrumb details-breadcrumb-img">
      <div class="container">
        <div class="dbread-top">
          <div class="row align-items-center">
            <div class="col-lg-8 col-sm-8">
              <div class="breadcrumb-top">
                <div>
                  <a href="/projects" target="_blank">Projects</a>
                </div>
                <% if (data.property_type){ %>
                  <div>
                    <span>/</span>
                  </div>
                  <div>
                    <a href="/properties?type=<%= data.property_type %>" target="_blank"><%= data.property_type %></a>
                  </div>
                <%}%>           
              </div>
              <h2 style="display: flex;">
                <span><%- include ./partials/bookmark.ejs%></span>
                <span class="ml-1"><%= data.name %></span>
              </h2>
              <div class="d-flex flex-column justify-content-start flex-md-row align-items-md-center">
                <div style="display: flex; flex:1" class="justify-content-start align-items-start">
                  <p class="">
                    <img src="/v1/img/gps.webp" alt="" />
                    <div class="text-white">
                      <% if (location.subcity){%>
                        <a class="clr-white text-white" href="<%=location.subcity.url%>" target="_blank">
                          <%= location.subcity.name %>
                        </a>,
                      <%}%>
                      <% if (location.city){%>
                        <a class="clr-white text-white" href="<%=location.city.url%>" target="_blank">
                          <%= location.city.name %>
                        </a>,
                      <%}%>
                      <% if (location.district){%>
                        <a class="clr-white text-white" href="<%=location.district.url%>" target="_blank">
                          <%= location.district.name %>
                        </a>,
                      <%}%>
                      <% if (location.state){%>
                        <a class="clr-white text-white" href="<%=location.state.url%>" target="_blank">
                          <%= location.state.name %>
                        </a>
                      <%}%>
                    </div>
                  </p>
                </div>
                
              </div>
              <div class="d-flex justify-content-start align-items-center mt-2">
                <% if ( data.price && data.price.min){ %>
                  <img src="/v1/img/price_tag.png" alt="" style="opacity: 1;  height: 20px; object-fit: contain; margin-right: 0px; filter:invert()"/>
                  <div class="d-flex flex-column flex-lg-row ml-2 align-items-center">
                    <div class="d-flex" style="font-size: 16px !important;">
                      <p class="text-left" style="font-size: inherit !important;">Starting :</p>
                      <p style="font-size: inherit !important;"><b class="px-2 text-left"> ₹ <%=changeNumberFormat(data.price.min)%></b></p>
                    </div>
                    <!-- </td> -->
                    <% if (emi_per_lac) { %>
                    <!-- <td> -->
                      <div class="d-none d-lg-flex justify-content-center px-3 py-1 ml-2 emi_outline">
                        <p class="ml-1 text-right">EMI Starting :</p>
                      
                        <p class="pl-2" ><b class="text-left"> ₹ <%=locals.formatToIndianNumber.format(Math.round(emi_per_lac * data.price.min / 100000))%><span type="button" class="btn btn-sm emi-tooltip btn-secondary" data-toggle="tooltip" data-html="true" title="<b>Down payment: 20%<br/>Loan for 25 yrs</b>">
                          <svg fill="#fff" version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="12px" height="12px" viewBox="0 0 416.98 416.98" xml:space="preserve" stroke="#fff" transform="matrix(1, 0, 0, 1, 0, 0)rotate(0)"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round" stroke="#CCCCCC" stroke-width="0.833958"></g><g id="SVGRepo_iconCarrier"> <g> <path d="M356.004,61.156c-81.37-81.47-213.377-81.551-294.848-0.182c-81.47,81.371-81.552,213.379-0.181,294.85 c81.369,81.47,213.378,81.551,294.849,0.181C437.293,274.636,437.375,142.626,356.004,61.156z M237.6,340.786 c0,3.217-2.607,5.822-5.822,5.822h-46.576c-3.215,0-5.822-2.605-5.822-5.822V167.885c0-3.217,2.607-5.822,5.822-5.822h46.576 c3.215,0,5.822,2.604,5.822,5.822V340.786z M208.49,137.901c-18.618,0-33.766-15.146-33.766-33.765 c0-18.617,15.147-33.766,33.766-33.766c18.619,0,33.766,15.148,33.766,33.766C242.256,122.755,227.107,137.901,208.49,137.901z"></path> </g> </g></svg>
                        </span></b></p>
                      </div>
                    <% } %>
                  </div>
                <%}%>
              </div>
              <% if (emi_per_lac && data.price && data.price.min) { %>
                <!-- <td> -->
                  <div class="d-flex d-lg-none justify-content-center px-2 py-1 my-2 emi_outline" style="width: fit-content;">
                    <p class="ml-1 text-right">EMI Starting :</p>
                  
                    <p class="pl-2" ><b class="text-left"> ₹ <%=locals.formatToIndianNumber.format(Math.round(emi_per_lac * data.price.min / 100000))%><span type="button" class="btn btn-sm emi-tooltip btn-secondary" data-toggle="tooltip" data-html="true" title="<b>Down payment: 20%<br/>Loan for 25 yrs</b>">
                      <svg fill="#fff" version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="12px" height="12px" viewBox="0 0 416.98 416.98" xml:space="preserve" stroke="#fff" transform="matrix(1, 0, 0, 1, 0, 0)rotate(0)"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round" stroke="#CCCCCC" stroke-width="0.833958"></g><g id="SVGRepo_iconCarrier"> <g> <path d="M356.004,61.156c-81.37-81.47-213.377-81.551-294.848-0.182c-81.47,81.371-81.552,213.379-0.181,294.85 c81.369,81.47,213.378,81.551,294.849,0.181C437.293,274.636,437.375,142.626,356.004,61.156z M237.6,340.786 c0,3.217-2.607,5.822-5.822,5.822h-46.576c-3.215,0-5.822-2.605-5.822-5.822V167.885c0-3.217,2.607-5.822,5.822-5.822h46.576 c3.215,0,5.822,2.604,5.822,5.822V340.786z M208.49,137.901c-18.618,0-33.766-15.146-33.766-33.765 c0-18.617,15.147-33.766,33.766-33.766c18.619,0,33.766,15.148,33.766,33.766C242.256,122.755,227.107,137.901,208.49,137.901z"></path> </g> </g></svg>
                    </span></b></p>
                  </div>
              <% } %>
              <!-- <p class="mt-2 d-flex flex-row justify-content-center- align-items-center">
                <img src="/images/views.webp"/>&nbsp;
                <span><%=data.views || 0%>&nbsp;views</span>
              </p> -->
            </div>
            <div class="col-lg-4 col-sm-4 p-0">
              <% if (data.is_live === "1") {%>
                <div class="notLiveGray">This page is under development and not live on website.</div> 
              <% } else if (data.is_live === "3" || data.is_live === "4"){%>
                <div class="notLive"> This project is not live anymore </div>
              <% } else {%>
              <div class="book-property-btn-wrp">
                <div class="col-lg-6 col-sm-6">
                  <div class="text-lg-right text-center">
                    <div class="dropdown show">
                      <a class="schedule-visit-btn mt-0" data-toggle="modal" data-target="#SCDVisitModal" href="#">
                        Schedule visit
                      </a>
                    </div>
                  </div>
                </div>
                <div class="col-lg-6 col-sm-6">
                <button class="btn btn-book-prop" style="margin-left: 0px;" onclick="scrollto('#all_properties_div')">View Properties</button>
                </div>
              </div>
              <% } %>
            </div>
            <div class="modal fade" id="SCDVisitModal" tabindex="-1" role="dialog" aria-labelledby="SCDVisitModallTitle" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered modal-lg auth-modal-mod scd-visit-modal" role="document">
                <div class="modal-content ">
                  <button class="btn auth-modal-close" type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                  <div class="modal-body p-0">
                    <div class="row">
                      <div class="col-lg-12 col-sm-12 p-0">
                        <div class="fdp-modal-left-content right-auth-md-content">
                          <div id="ScheduleVisit">
                            <h2>Schedule Appointment</h2>
                            <form id="schedule-visit-form">
                              <div class="form-group">
                                <input class="form-control" type="text" placeholder="Full Name" id="schedule-name">
                              </div>
                              <div class="form-group row justify-content-between position-relative w-100 mx-0">
                                <div onclick="showPhoneCodeOptions('schedule-phone-code', 'schedule-phone-code-name', 'schedule-phone-code-list', 'schedule-phone-code-search')" class="position-relative form-control col-3 p-0 ps-1 d-flex justify-content-around align-items-center" style="height: fit-content; background-color: #d9d9df;">
                                  <input type="text" class="w-100 mb-0 bg-white border-0 text-center" name="countryCode"  id="schedule-phone-code" value="+91" disabled required style="margin-bottom: 0px !important; background-color: #d9d9df !important;">
                                  <input id="schedule-phone-code-search" type="text" placeholder="India" class="phone-code-search w-100 p-2 mb-0 bg-white border-0 text-center" style="display: none;">
                                  <input type="text" class="w-100 d-none" name="countryName"  id="schedule-phone-code-name" value="India" disabled required>
                                  <svg class="m-1" stroke="currentColor" fill="none" stroke-width="2" viewBox="0 0 20 20" stroke-linecap="round" stroke-linejoin="round" height="18" width="18" xmlns="http://www.w3.org/2000/svg"><polyline points="6 9 12 15 18 9"></polyline></svg>
                                </div>
                                <div id="schedule-phone-code-list" class="phone-code-list position-absolute bottom-0" style="display: none;">
                                  <div class="w-100 p-2">
                                  </div>
                                  <table class="p-2"></table>
                                </div>
                                <input class="form-control col-8" type="text" onkeypress="return event.keyCode === 8 || event.charCode >= 48 && event.charCode <= 57" placeholder="Phone Number" id="schedule-phone" required>
                              </div>
                              <div class="form-group">
                                <input class="form-control" type="email" placeholder="Email" id="schedule-email">
                              </div>                                                                                 
                              <div class="row">
                                <div class="col-lg-6 pr-lg-2 pr-sm-2">
                                  <div class="form-group">
                                    <div class="input-type-date-icon">
                                      <input class="form-control" type="date" id="schedule-date" placeholder="DD-MM-YYYY">
                                      <div class="artifical-input-feild">
                                        <p id="calender-value">YYYY-MM-DD</p>
                                        <img src="/v1/img/cal-icon.webp" class="img img-fluid" alt="">
                                      </div>
                                    </div>
                                  </div>
                                </div>
                                <div class="col-lg-6 pl-lg-2 pl-sm-2">
                                  <div class="form-group">
                                    <select class="form-control" type="text" placeholder="date" id="schedule-time">
                                      <option value="0" selected> Time</option>
                                      <option value="First Half (8AM to 12PM)"> First Half (8AM to 12PM)</option>
                                      <option value="Second Half (12PM to 4PM)"> Second Half (12PM to 4PM)</option>
                                      <option value="Third Half (4PM to 8PM)"> Third Half (4PM to 8PM)</option>
                                    </select>
                                  </div>
                                </div>
                              </div>
                              <div class="form-group">
                                <select id="interested-other-projects" class="form-control">
                                  <option value="Interested in other Projects" selected>Are you interested in other project/property in same locality?</option>
                                  <option value="yes">Yes</option>
                                  <option value="no">No</option>
                                </select>
                              </div>
                              <div class="form-group d-none">
                                <input class="form-control" type="text" value="<%= data.name %>" id="schedule-project">
                              </div>
                              <div class="form-group d-none">
                                <input class="form-control" type="text" value="<%= data.url %> " id="schedule-url">
                              </div>
                              <div class="d-flex justify-content-start align-items-center mb-3">
                                <input style="width: fit-content;" class="mb-0" type="checkbox" id="newsletter-check-sv" checked>
                                <label class="ml-2 mb-0" for="newsletter-check-sv"> Subscribe to AssetZilla Newsletter </label>
                              </div>
                              <div>
                                <button type="submit" class="btn auth-modal-btn">Confirm</button>
                              </div>
                            </form>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- --------------------------------------------------------------- -->
  <div class=" common-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-md-4 col-8">
          <div class="w-100 d-flex flex-row x-views my-3">
            <p class="text-center mb-0 py-1 d-flex flex-row justify-content-start align-items-center">
              <img src="/images/views.webp" class="viewed-x-times-div-img">
              <span>&nbsp;<%=data.views || 0%>&nbsp;Page <% if(data.views > 1) {%>views <%} else {%>view<%}%>&nbsp;</span>
            </p>
          </div>
        </div>
        <div class="mobile-view-map col-md-8 col-4">
          <a href="#side_map">View Map</a>
        </div>
      </div>
    </div>
    <%- include ./partials/media-galary.ejs%>
    <!-- <div class="container">
      <div class="w-100 flex-row viewed-x-times-div mb-3 mt-0">
        <p class="text-center mb-0 py-1 d-flex flex-row justify-content-center align-items-center">
          <img src="/images/views.webp" class="viewed-x-times-div-img">
          &nbsp;This Page has been viewed 
          <span>&nbsp;<%=data.views || 0%>&nbsp;times.</span>
        </p>
      </div>
    </div> -->
  </div>
  <!-- ------------------------------------------------------------------- -->
  <div class="details-box-wrapper">
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <div class="common-box-dt">
            <div class="dt-common-head">
              <h2>Description</h2>
            </div>
            <div class="">
              <% if (data.description){ %>
                <h6><%- data.description %></h6>
              <%}%>
            </div>
            <% if (builder.length > 0){ %>
              <div class="row mt-4 pt-2 mb-3">
                <div class="col-6 col-lg-3">
                  <div>
                    <h5>Builder :</h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <a href="<%= builder[0].url %>" target="_blank">
                      <img class="img img-fluid desc-logo-img" src="<%=locals.CURRENT_IMAGE_BASE_URL%>/builder/<%= builder[0].logo %>" alt="">
                    </a>
                  </div>
                </div>
              </div>
            <%}%>
            <% if (data.authority){ %>
              <div class="row mb-3">
                <div class="col-6  col-lg-3">
                  <div>
                    <h5>Approved by : </h5>
                  </div>
                </div>
                <div class="col-6 col-lg-9">
                  <div>
                    <h5>
                      <% if (data.authortiy_url){ %>
                      <a href="<%= data.authortiy_url %>">
                        <%= data.authority %></a>
                      <%} else{ %>
                      <%= data.authority %>
                      <%}%>
                    </h5>
                  </div>
                </div>
              </div>
            <%}%>
            <div class="row mb-3">
              <div class="col-6  col-lg-3">
                <div>
                  <h5>Project Name :</h5>
                </div>
              </div>
              <div class="col-6 col-lg-9">
                <div>
                  <h5><b> <%= data.name %></b></h5>
                </div>
              </div>
            </div>
          </div>
          <% if (data.details && data.details.length > 0){ %>
            <div class="common-box-dt">
              <div class="dt-common-head">
                <h2>Properties Available</h2>
              </div>
              <div class="row">
                <div class="col-lg-12">
                  <div class="apart-table table-responsive p-0 mb-0">
                    <table class="table">
                      <% if (data.property_type && data.property_type === "Residential Plots"){ %>
                        <thead>
                          <tr>
                            <th scope="col">Type</th>
                            <th scope="col">
                              Area
                              <span>
                                <select class="form-control form-control-sm unit-changer" id="convert-to" onchange="unitConverter(<%=JSON.stringify(data.details)%>, 'sq ft', 'area-<%= [i] %>', this.value )">
                                  <option selected>sq ft</option>
                                  <option>acre</option>
                                  <option>sq yards</option>
                                  <option>sq meter</option>
                                </select>
                              </span>
                            </th>
                            <th scope="col">Total Units</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0; i < data.details.length; i++) { %>
                            <tr>
                              <td><%=data.details[i].name %></td>
                              <td>
                                <span id="area-<%= [i]%>%>">
                                  <%=data.details[i].area %>
                                </span>
                              </td>
                              <td>
                                <%=data.details[i].count %>
                              </td>
                            </tr>
                          <%}%>
                        </tbody> 
                      <%}%>
                      <% if (data.property_type && data.property_type === "Villas"){ %>
                        <thead>
                          <tr>
                            <th scope="col">Type</th>
                            <th scope="col">Build Up Area
                              <span>
                                <select class="form-control form-control-sm unit-changer" id="convert-to" onchange="unitConverter(<%=JSON.stringify(data.details)%>, 'sq ft', 'area-<%= [i] %>', this.value )">
                                  <option selected>sq ft</option>
                                  <option>acre</option>
                                  <option>sq yards</option>
                                  <option>sq meter</option>
                                </select>
                              </span>
                            </th>
                            <th scope="col"> Land Area</th>
                            <th scope="col">Total Units</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0; i < data.details.length; i++) { %>
                          <tr>
                            <td><%=data.details[i].unit_size %></td>
                            <td>
                              <span id="area-<%= [i]%>%>">
                                <%=data.details[i].area %>
                              </span>
                            </td>
                            <td>
                              <span id="land-area-<%= [i]%>%>">
                                <%=data.details[i].land_area %> </span>
                            </td>
                            <td>
                              <%=data.details[i].count %>
                            </td>
                          </tr>
                          <%}%>
                        </tbody> 
                      <%}%>
                      <% if (data.property_type && data.property_type === "Apartments"){ %>
                        <thead>
                          <tr>
                            <th scope="col">Type</th>
                            <th scope="col">
                              <div class="d-flex align-items-center">
                                Area
                                <span>
                                  <select class="form-control form-control-sm unit-changer ml-2" id="convert-to" onchange="unitConverter(<%=JSON.stringify(data.details)%>, 'sq ft', 'area-<%= [i] %>', this.value )">
                                    <option selected>sq ft</option>
                                    <option>acre</option>
                                    <option>sq yards</option>
                                    <option>sq meter</option>
                                  </select>
                                </span>
                              </div>
                            </th>
                            <th scope="col">Total Units</th>
                            <th>Additional Rooms</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0; i < data.details.length; i++) { %>
                            <tr>
                              <td><%=data.details[i].unit_size %></td>
                              <td>
                                <span id="area-<%= [i]%>%>">
                                  <%=data.details[i].area %>
                                </span>
                              </td>
                              <td>
                                <%=data.details[i].count %>
                              </td>
                              <td>
                                <% if (data.details[i] && data.details[i].additional_rooms && data.details[i].additional_rooms.length > 0){ %>

                                <% for(var x=0; x < data.details[i].additional_rooms.length; x++) { %>
                                <%=data.details[i].additional_rooms[x]%><br>
                                <%}%>
                                                          <%}%>
                              </td>
                            </tr>
                          <%}%>
                        </tbody> 
                      <%}%>
                      <% if (data.property_type && data.property_type === "Floors"){ %>
                        <thead>
                          <tr>
                            <th scope="col">Type</th>
                            <th scope="col">Apartment Area
                              <span>
                                <select class="form-control form-control-sm unit-changer" id="convert-to" onchange="unitConverter(<%=JSON.stringify(data.details)%>, 'sq ft', 'area-<%= [i] %>', this.value )">
                                  <option selected>sq ft</option>
                                  <option>acre</option>
                                  <option>sq yards</option>
                                  <option>sq meter</option>
                                </select>
                              </span>
                            </th>
                            <th scope="col">Land Area
                            <th scope="col">Total Units</th>
                            <th>Additional Rooms</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i=0; i < data.details.length; i++) { %>
                            <tr>
                              <td><%=data.details[i].unit_size %></td>
                              <td>
                                <span id="area-<%= [i]%>%>">
                                  <%=data.details[i].area %>
                                </span>
                              </td>

                              <td>
                                <span id="apartment_area-<%= [i]%>%>"><%=data.details[i].apartment_area %></span>
                              </td>
                              <td>
                                <%=data.details[i].count %>
                              </td>
                              <td>
                                <% if (data.details[i] && data.details[i].additional_rooms && data.details[i].additional_rooms.length > 0){ %>

                                <% for(var x=0; x < data.details[i].additional_rooms.length; x++) { %>
                                <%=data.details[i].additional_rooms[x]%><br>
                                <%}%>
                                                          <%}%>
                              </td>
                            </tr>
                          <%}%>
                        </tbody> 
                      <%}%>
                      <% if (data.property_type && data.property_type === "Commercial Land"){ %>
