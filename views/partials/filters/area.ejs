<strong>Area</strong>
<div class="d-flex flex-column bd-highlight mt-2">
    <div class="pretty p-default">
        <input type="checkbox" id="area-0-to-1000-sq-ft" onchange="handleFilters('area','0 to 1000 sq ft')" />
        <!-- <input type="checkbox" id="area-0-1000-sq-ft" onchange="handleFilters('area','< 1000 sq ft')" /> -->
        <div class="state">
            <label> 0 - 1000 sq ft.<span id="zeroToTenCount"></span></label> 
        </div> 
    </div> 
</div>
<div class="d-flex flex-column bd-highlight mt-2">
    <div class="pretty p-default">
        <input type="checkbox" id="area-1000-to-1500-sq-ft" onchange="handleFilters('area','1000 to 1500 sq ft')" />
        <!-- <input type="checkbox" id="area-1000-1500-sq-ft" onchange="handleFilters('area','1000 - 1500 sq ft')" /> -->
                        <div class="state">
                            <label>1000 - 1500 sq ft<span id="tenToFifteenCount"></span></label>
                        </div>
                    </div>
        </div>
        <div class="d-flex flex-column bd-highlight mt-2">
            <div class="pretty p-default">
                <input type="checkbox" id="area-1500-to-2000-sq-ft" onchange="handleFilters('area','1500 to 2000 sq ft')" />
                <!-- <input type="checkbox" id="area-1500-2000-sq-ft" onchange="handleFilters('area','1500-2000 sq ft')" /> -->
                <div class="state">
                    <label>1500-2000 sq ft<span id="fifteenToTwentyCount"></span></label>
                </div>
            </div>
        </div>
        <div class="d-flex flex-column bd-highlight mt-2">
            <div class="pretty p-default">
                <input type="checkbox" id="area-2000-sq-ft-and-above" onchange="handleFilters('area','2000 sq ft and above')" />
                <!-- <input type="checkbox" id="area-2000-sq-ft" onchange="handleFilters('area','>2000 sq ft')" /> -->
                <div class="state">
                    <label>2000 sq ft and above<span id="twentyToAboveCount"></span></label>
                </div>
            </div>
</div>

<script>

     $.post('/api/getFilterData/area',{ query: JSON.stringify(filter_state_for_project)}, function (data) {
        if(data.success) {
            Object.keys(data.data).forEach(function(key) {
                document.getElementById(`${key}`).innerHTML = ` (${data.data[key]})` 
            })
        }
    })
</script>