<% for(var i=0; i < results.length; i++) { %>
<div class="col-md-4">
    <div class="card mb-3 listing-card" onclick=go_to_page("<%=results[i].url %>")>
        <div class="row no-gutters shadow">
            <div class="col-md-12">

                <img src="<%=results[i].logo %>" class="card-img" alt="">

                <div class="card-body">

                    <div class="d-flex bd-highlight">
                        <div class="p-2 w-100 bd-highlight">
                            <h5 class="card-title"><%=results[i].name %></h5>
                            <small class="light-grey-clr">
                                <i class="fas fa-map-marker-alt"></i>
                                <% if (results[i].subcity){ %><%=results[i].subcity %>,<%}%><% if (results[i].city){ %><%=results[i].city %> <%}%>
                                        </small>
                                    </div>
                                </div>

                                <div class="d-flex d-flex align-content-start flex-wrap bd-highlight mb-3">
                                        <% if (results[i].total_projects){ %>
                                            <!-- <div class="p-2 bd-highlight">
                                                <i class="fas fa-building mr-2"></i>
                                                <strong>Total Projects : </strong><%=results[i].total_projects %>
                                            </div>

                                             -->

                                            <%}%>
                                            <div class="p-2 bd-highlight">
                                                <i class="fas fa-building mr-2"></i>
                                                <strong>Total Projects : </strong><%=results[i].total_projects %>
                                            </div>

                                    
                                </div>
                                <div class="pl-2" style="margin-top: -22px !important">
                                        <div>
                                                <% if (results[i].project_status_count && results[i].project_status_count.ReadyToMove){ %>
                                                    <span style="mr-2">
                                                        <strong>Ready To Move: </strong><%=results[i].project_status_count.ReadyToMove %>
                                                    </span>
                                                    <%}%>

                                                    <% if (results[i].project_status_count && results[i].project_status_count.UnderConstruction){ %>
                                                        <span style="mr-2"><strong>Under Construction: </strong><%=results[i].project_status_count.UnderConstruction %></span>
                                                        <%}%>

                                                        <% if (results[i].project_status_count && results[i].project_status_count.PreLaunch){ %>
                                                            <span style="mr-2"><strong>Pre Launch: </strong><%=results[i].project_status_count.PreLaunch %></span>
                                                            <%}%>
                                                            <% if (results[i].price && results[i].price.min){ %>
                                                                <span style="mr-2"><strong>Properties starting: </strong><%=results[i].price.min.toLocaleString(undefined,{ minimumFractionDigits: 2 }) %></span>
                                                                    <%}%>
                                        </div>
                                        <div>
                                                
                                        </div>
                                        <div>
                                                
                                        </div>
                                        
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>